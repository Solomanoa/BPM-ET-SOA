<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Compagniesuurance" table="Compagniesuurance" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="notification" column="NotificationID" class="Notification" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="id_compagnie" column="Id_compagnie" type="string" length="255" not-null="false" lazy="false"/>
		<property name="nom_compagnie" column="Nom_compagnie" type="string" length="255" not-null="false" lazy="false"/>
		<property name="adresse_compagnie" column="Adresse_compagnie" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
