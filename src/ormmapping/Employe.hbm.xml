<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Employe" table="Employe" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="notification" column="NotificationID" class="Notification" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="informationPersonnelle" column="InformationPersonnelleID" class="InformationPersonnelle" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nom" column="Nom" type="string" length="255" not-null="false" lazy="false"/>
		<property name="matricule" column="Matricule" type="string" length="255" not-null="false" lazy="false"/>
		<one-to-one name="changementBeneficiaire" class="ChangementBeneficiaire" cascade="save-update,lock" property-ref="employe" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
