<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Beneficiaire" table="Beneficiaire" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="changementBeneficiaire" column="ChangementBeneficiaireID" class="ChangementBeneficiaire" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nom" column="Nom" type="string" length="255" not-null="false" lazy="false"/>
		<property name="lienParente" column="LienParente" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
