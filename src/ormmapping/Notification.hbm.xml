<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Notification" table="Notification" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="consultantRH" column="ConsultantRHID" class="ConsultantRH" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="notification" column="Notification" type="string" length="255" not-null="false" lazy="false"/>
		<one-to-one name="compagniesuurance" class="Compagniesuurance" cascade="save-update,lock" property-ref="notification" lazy="proxy" access="field"/>
		<one-to-one name="employe" class="Employe" cascade="save-update,lock" property-ref="notification" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
