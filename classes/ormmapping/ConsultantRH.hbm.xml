<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ConsultantRH" table="ConsultantRH" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nom" column="Nom" type="string" length="255" not-null="false" lazy="false"/>
		<one-to-one name="changementBeneficiaire" class="ChangementBeneficiaire" cascade="save-update,lock" property-ref="consultantRH" lazy="proxy" access="field"/>
		<one-to-one name="notification" class="Notification" cascade="save-update,lock" property-ref="consultantRH" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
