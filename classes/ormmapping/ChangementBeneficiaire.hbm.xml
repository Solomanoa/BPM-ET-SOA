<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ChangementBeneficiaire" table="ChangementBeneficiaire" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="consultantRH" column="ConsultantRHID" class="ConsultantRH" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="employe" column="EmployeID" class="Employe" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="ancien_beneficiare" column="Ancien_beneficiare" type="string" length="255" not-null="false" lazy="false"/>
		<property name="nouveau_beneficiaire" column="Nouveau_beneficiaire" type="string" length="255" not-null="false" lazy="false"/>
		<property name="date_changement" column="Date_changement" type="date" not-null="false" lazy="false"/>
		<one-to-one name="beneficiaire" class="Beneficiaire" cascade="save-update,lock" property-ref="changementBeneficiaire" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
