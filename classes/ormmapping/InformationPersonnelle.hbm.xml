<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="InformationPersonnelle" table="InformationPersonnelle" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="addresse" column="Addresse" type="string" length="255" not-null="false" lazy="false"/>
		<property name="tel" column="Tel" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="dateNais" column="DateNais" type="date" not-null="false" lazy="false"/>
		<property name="cin" column="Cin" type="string" length="255" not-null="false" lazy="false"/>
		<one-to-one name="employe" class="Employe" cascade="save-update,lock" property-ref="informationPersonnelle" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
